<div class="container px-10">
  <table class="table-auto">
    <tbody>
      <% @books.each do |book| %>
        <tr>
          <td><%= image_tag book.medium_image_url %><td>
          <td><%= book.title %><td>
          <td><%= book.author %></td>
          <td><%= book.item_price %><td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
